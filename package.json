{
  "name": "Pantry",
  "version": "0.0.1",
  "author": "Rohan Likhite <rlikhite@gmail.com>",
  "license": "MIT",
  "description": "Free JSON storage for small projects",
  "main": "src/main.ts",
  "repository": {
    "url": "https://github.com/imRohan/Pantry",
    "type": "git"
  },
  "scripts": {
    "dev": "concurrently -k \"tsc && node dist/src/main.js\" \"webpack --config webpack.dev.js\"",
    "dev-server": "tsc && node --inspect=[0.0.0.0] dist/src/main.js",
    "linter": "eslint -c .eslintrc.js --ext .ts src/",
    "fix-linting": "eslint -c .eslintrc.js --ext .ts src/ --fix",
    "tests": "jest --silent --verbose --coverage",
    "build": "webpack --config webpack.prod.js",
    "validate": "jest && eslint -c .eslintrc.js --ext .ts src/",
    "server": "tsc && node dist/src/main.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "<rootDir>/node_modules/ts-jest"
    },
    "testRegex": "./tests/.*.test.ts$",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ]
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^10.17.21",
    "@types/pino": "^5.20.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^8.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "html-loader": "^0.5.5",
    "jest": "^29.5.0",
    "pino-pretty": "^3.6.1",
    "sass": "^1.49.9",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.21.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "typescript": "^4.0.5",
    "vue-loader": "^15.9.2",
    "vue-resource": "^1.5.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@sendgrid/mail": "^7.1.1",
    "airtable": "^0.8.1",
    "axios": "^0.21.2",
    "body-parser": "^1.18.3",
    "class-validator": "^0.11.1",
    "cors": "^2.8.4",
    "deepmerge": "^4.2.2",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "helmet": "^3.22.0",
    "pino": "^5.17.0",
    "pino-logflare": "^0.3.12",
    "redis": "^3.1.1",
    "url-loader": "^1.0.1",
    "uuid": "^3.4.0",
    "vue": "^2.5.16",
    "vue-json-editor": "^1.4.3"
  }
}
